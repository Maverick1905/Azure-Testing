{
	"name": "CMTDataFlow",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "CMTSampleInput",
						"type": "DatasetReference"
					},
					"name": "HumanResourcesTable"
				},
				{
					"dataset": {
						"referenceName": "CMTSampleInput",
						"type": "DatasetReference"
					},
					"name": "HumanResourceTable"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "CMTSampleOutput",
						"type": "DatasetReference"
					},
					"name": "CMTresult",
					"description": "CMTexport",
					"rejectedDataLinkedService": {
						"referenceName": "CMTResult",
						"type": "LinkedServiceReference"
					}
				}
			],
			"transformations": [
				{
					"name": "OneColumnVortag"
				},
				{
					"name": "OneColumnTag"
				},
				{
					"name": "join1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          DepartmentID as integer,",
				"          Name as string,",
				"          GroupName as string,",
				"          ModifiedDate as timestamp",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> HumanResourcesTable",
				"source(output(",
				"          DepartmentID as integer,",
				"          Name as string,",
				"          GroupName as string,",
				"          ModifiedDate as timestamp",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> HumanResourceTable",
				"HumanResourcesTable select(mapColumn(",
				"          DepartmentID",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> OneColumnVortag",
				"HumanResourceTable select(mapColumn(",
				"          DepartmentID",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> OneColumnTag",
				"OneColumnVortag, OneColumnTag join(OneColumnVortag@DepartmentID == OneColumnTag@DepartmentID,",
				"     joinType:'inner',",
				"     matchType:'exact',",
				"     ignoreSpaces: false,",
				"     broadcast: 'auto')~> join1",
				"join1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     partitionFileNames:['CMT'],",
				"     truncate: true,",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     partitionBy('hash', 1)) ~> CMTresult"
			]
		}
	}
}